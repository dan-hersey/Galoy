  <!-- vs Sidecar -->
  <section class="section section-alt">
    <div class="container container-sm">
      <h2 class="text-center mb-8">Primary Core vs. Sidecar Model</h2>

      <div class="card" style="padding: 0; overflow: hidden;">
        <table style="width: 100%; border-collapse: collapse;">
          <thead>
            <tr style="background: var(--neutral-900); color: white;">
              <th style="padding: var(--space-4); text-align: left; font-weight: 600;"></th>
              <th style="padding: var(--space-4); text-align: center; font-weight: 600;">Primary Core</th>
              <th style="padding: var(--space-4); text-align: center; font-weight: 600;">Sidecar Model</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td style="padding: var(--space-4); border-bottom: 1px solid var(--border-default);">Best for</td>
              <td style="padding: var(--space-4); border-bottom: 1px solid var(--border-default); text-align: center;">De novo banks, Bitcoin-first institutions</td>
              <td style="padding: var(--space-4); border-bottom: 1px solid var(--border-default); text-align: center;">Existing banks adding Bitcoin</td>
            </tr>
            <tr>
              <td style="padding: var(--space-4); border-bottom: 1px solid var(--border-default);">Existing core required?</td>
              <td style="padding: var(--space-4); border-bottom: 1px solid var(--border-default); text-align: center;"><span style="color: #10b981; font-weight: 600;">No</span></td>
              <td style="padding: var(--space-4); border-bottom: 1px solid var(--border-default); text-align: center;"><span style="color: var(--color-accent); font-weight: 600;">Yes</span></td>
            </tr>
            <tr>
              <td style="padding: var(--space-4); border-bottom: 1px solid var(--border-default);">Bitcoin integration depth</td>
              <td style="padding: var(--space-4); border-bottom: 1px solid var(--border-default); text-align: center;">Native, foundational</td>
              <td style="padding: var(--space-4); border-bottom: 1px solid var(--border-default); text-align: center;">Add-on alongside existing</td>
            </tr>
            <tr>
              <td style="padding: var(--space-4); border-bottom: 1px solid var(--border-default);">Implementation time</td>
              <td style="padding: var(--space-4); border-bottom: 1px solid var(--border-default); text-align: center;">3-6 months</td>
              <td style="padding: var(--space-4); border-bottom: 1px solid var(--border-default); text-align: center;">2-4 months</td>
            </tr>
            <tr>
              <td style="padding: var(--space-4);">Fiat banking capabilities</td>
              <td style="padding: var(--space-4); text-align: center;">Yes, with integrations</td>
              <td style="padding: var(--space-4); text-align: center;">Existing core handles</td>
            </tr>
          </tbody>
        </table>
      </div>

